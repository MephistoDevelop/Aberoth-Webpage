<div class="contacto-container">
<% if logged_in? %>
    <div class="container">
      <p><span class="bolder">Nombre: </span><%= @user.name %> </p>
      <p> Apellidos: <%= @user.lastname %> </p>
      <p> Edad: <%= @user.age %></p>
      <p> Correo:<%= @user.email %></p>
      <p> Password: <%= @user.password_digest %></p>
    </div>
<%else%>
    <% render 'shared/login_error' %>
<% end %>
</div>

